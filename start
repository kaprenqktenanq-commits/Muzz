rm -f logs.txt log.txt
rm -rf __pycache__
rm -f *.session
echo "Disabling IPV6"
sysctl -w net.ipv6.conf.all.disable_ipv6=1 &
sysctl -w net.ipv6.conf.default.disable_ipv6=1 &
sysctl -w net.ipv6.conf.lo.disable_ipv6=1 & 
sleep 2
echo "Starting Bot..."
# Run with lower CPU priority and unbuffered output to improve stability and logs
exec nice -n 5 python3 -u -m ArmedMusic
